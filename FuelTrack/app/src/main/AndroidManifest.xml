<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bashir1.fueltrack">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:name="bashir1.fueltrack.FuelTrackApplication"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEntryActivity"
            android:label="@string/title_activity_add_entry" />
        <activity
            android:name=".EditEntryActivity"
            android:label="@string/title_activity_edit_entry"></activity>
    </application>

</manifest>
